<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Permiso necesario para usar la vibraciÃ³n del dispositivo -->
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/dado_inicio_con_blanco"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dado_inicio_con_blanco"
        android:supportsRtl="true"
        android:theme="@style/Theme.MasterRollerDice">
        <activity
            android:name=".StartActivity"
            android:exported="true"
            android:theme="@style/Base.Theme.MasterRollerDice">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:theme="@style/Base.Theme.MasterRollerDice" />

    </application>

</manifest>